{
	"name": "@trysoma/sdk",
	"version": "0.0.2-snapshot-1",
	"main": "./dist/index.js",
	"type": "module",
	"module": "./dist/index.js",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./bridge": {
			"import": "./dist/bridge.js",
			"types": "./dist/bridge.d.ts"
		},
		"./agent": {
			"import": "./dist/agent.js",
			"types": "./dist/agent.d.ts"
		},
		"./patterns": {
			"import": "./dist/patterns.js",
			"types": "./dist/patterns.d.ts"
		},
		"./vite": {
			"import": "./dist/vite.js",
			"types": "./dist/vite.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"typings": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepare": "npm run build",
		"lint": "biome check . && tsc --noEmit",
		"lint:ci": "biome ci . && tsc --noEmit",
		"lint:fix": "biome check --write . && tsc --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@a2a-js/sdk": "^0.3.4",
		"@openapitools/openapi-generator-cli": "^2.15.3",
		"@restatedev/restate-sdk": "^1.9.0",
		"@trysoma/api-client": "workspace:*",
		"@trysoma/sdk-core": "workspace:*",
		"zod": "^4.1.12",
		"zod-to-json-schema": "^3.24.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/node": "^22.18.10",
		"vite": "^7.1.12",
		"vitest": "^4.0.8"
	},
	"peerDependencies": {
		"typescript": "^5",
		"vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
	}
}

{
	"name": "@trysoma/api-client",
	"version": "0.0.0",
	"description": "OpenAPI client for @trysoma/api-client",
	"author": "OpenAPI-Generator",
	"repository": {
		"type": "git",
		"url": "https://github.com/GIT_USER_ID/GIT_REPO_ID.git"
	},
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"module": "./dist/esm/index.js",
	"sideEffects": false,
	"scripts": {
		"build": "tsc && tsc -p tsconfig.esm.json",
		"prepare": "npm run build",
		"lint": "biome check . && tsc --noEmit",
		"lint:ci": "biome ci . && tsc --noEmit",
		"lint:fix": "biome check --write --unsafe . && tsc --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"typescript": "^4.0 || ^5.0",
		"vitest": "^4.0.8"
	}
}

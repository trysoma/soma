{
	"compileOnSave": true,
	"include": ["**/*.ts"],
	"exclude": ["dist", "node_modules"],
	"compilerOptions": {
	  "module": "es2022",
	  "moduleResolution": "node",
	  "noImplicitReturns": true,
	  "outDir": "dist",
	  "sourceMap": true,
	  "strict": true,
	  "target": "es2022",
	  "skipLibCheck": true,
	  "esModuleInterop": true
	}
  }
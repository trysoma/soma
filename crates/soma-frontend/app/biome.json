{
	"$schema": "https://biomejs.dev/schemas/2.2.5/schema.json",
	"extends": ["../../../biome.json"],
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"files": {
		"ignoreUnknown": true
	},
	"formatter": {
		"indentWidth": 2
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noUnknownAtRules": "off"
			}
		},
		"domains": {
			"react": "recommended"
		}
	}
}
